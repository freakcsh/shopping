<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="CheckBoxTheme" parent="Widget.AppCompat.CompoundButton.CheckBox">
        <item name="android:button">@drawable/check_box_custom</item>
    </style>

    <style name="fiveRatingBar" parent="Widget.AppCompat.RatingBar.Indicator">
        <item name="android:progressDrawable">@drawable/five_rating_bar</item>
        <item name="android:minHeight">20dp</item>
        <item name="android:maxHeight">20dp</item>
    </style>
</resources>
